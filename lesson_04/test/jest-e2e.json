{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": ["node_modules", "test"],
  "coverageDirectory": "./coverage",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^test-utils/(.*)$": "<rootDir>/test/test-utils/$1"
  }
}